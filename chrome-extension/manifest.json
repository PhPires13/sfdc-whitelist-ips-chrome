{
    "name": "Beautify Salesforce Debug Logs",
    "version": "0.1.2",
    "manifest_version": 2,
    "description": "Beautify Salesforce Debug Logs https://github.com/motiko/sfdc-debug-logs",
    "content_scripts": [
        {
            "css": [
                "debug.css"
            ],
            "matches": [
                "https://*.salesforce.com/p/setup/layout/ApexDebugLogDetailEdit/*"
            ],
            "js": [
                "beautify.js",
                "sfdc.debug.js"
            ],
            "run_at": "document_end"
        },
        {
            "matches": [
                "https://*.salesforce.com/setup/ui/listApexTraces.apexp*"
            ],
            "js": [
                "sfdc.debug.btn.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "https://*.force.com/*",
        "https://*.salesforce.com/*",
        "storage"
    ]
}