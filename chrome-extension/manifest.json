{
    "name": "Salesforce Debug Tools",
    "version": "0.1.9",
    "manifest_version": 2,
    "author": "Moti Korets",
    "icons": { "16": "16.png",
           "48": "48.png",
          "128": "128.png" },
    "description": "Salesforce debug tools",
    "content_scripts": [
        {
            "css": [
                "debug.css"
            ],
            "matches": [
                "https://*.salesforce.com/p/setup/layout/ApexDebugLogDetailEdit/*"
            ],
            "js": [
                "beautify.js",
                "sfdc.debug.js"
            ],
            "run_at": "document_end"
        },
        {
            "matches": [
                "https://*.salesforce.com/setup/ui/listApexTraces.apexp*"
            ],
            "js": [
                "sfdc.debug.btn.js"
            ],
            "run_at": "document_end"
        },
        {
            "matches": [
                "https://*.force.com/*",
                "https://*.salesforce.com/*"
            ],
            "js": [
                "mousetrap.min.js",
                "shortcuts.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "https://*.force.com/*",
        "https://*.salesforce.com/*"
    ]
}